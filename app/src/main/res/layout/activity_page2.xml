        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.332"
        app:srcCompat="@drawable/aviso" />

    <Button
        android:id="@+id/Bt_asistir_plan3"
        android:layout_width="143dp"
        android:layout_height="50dp"
        android:backgroundTint="#052A4C"
        android:text="ASISTIR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <Button
        android:id="@+id/Bt_plan4"
        android:layout_width="333dp"
        android:layout_height="175dp"
        android:backgroundTint="#939FA7"
        android:gravity="left"
        android:text="Plan #4"
        android:textSize="30sp"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.699" />

    <TextView
        android:id="@+id/Descr_plan4"
        android:layout_width="125dp"
        android:layout_height="83dp"
        android:text="HORARIO: CONTENIDO: UBICACIÓN: ASISTENTES:"
